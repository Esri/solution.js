{
  "name": "arcgis-clone-js",
  "version": "0.3.0",
  "description": "Transfer sets of ArcGIS Online items from one organization to another",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:node && npm run test:chrome",
    "test:chrome": "karma start --single-run --browsers=Chrome",
    "test:firefox": "karma start --single-run --browsers=Firefox",
    "test:node": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "docs": "typedoc --out ./docs/src/ src/index.ts",
    "build:esm": "tsc --module ES2015 --outDir ./dist/esm --declaration",
    "build:cjs": "tsc --module CommonJS --outDir ./dist/cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArcGIS/arcgis-clone-js.git"
  },
  "keywords": [],
  "author": "Esri",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ArcGIS/arcgis-clone-js/issues"
  },
  "homepage": "https://github.com/ArcGIS/arcgis-clone-js#readme",
  "devDependencies": {
    "@esri/arcgis-rest-auth": "^1.13.2",
    "@esri/arcgis-rest-common-types": "^1.13.2",
    "@esri/arcgis-rest-feature-service-admin": "^1.13.2",
    "@esri/arcgis-rest-groups": "^1.13.2",
    "@esri/arcgis-rest-items": "^1.13.2",
    "@esri/arcgis-rest-request": "^1.13.2",
    "@esri/arcgis-rest-sharing": "^1.13.2",
    "@types/arcgis-js-api": "^4.9.0",
    "@types/fetch-mock": "^6.0.4",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/jasmine": "^2.8.9",
    "@types/node": "^10.11.7",
    "coveralls": "^3.0.2",
    "fetch-mock": "^7.0.7",
    "jasmine": "^3.2.0",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coveralls": "^2.1.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.2",
    "karma-requirejs": "^1.1.0",
    "karma-typescript": "^3.0.13",
    "karma-typescript-es6-transform": "^1.0.4",
    "node-fetch": "^2.2.0",
    "requirejs": "^2.3.6",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.2"
  },
  "dependencies": {
    "es6-promise": "^4.2.5",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-form-data": "^2.0.0",
    "tslib": "^1.9.3"
  }
}
